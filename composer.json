{
  "name": "PGWireless/hotpot",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/PGWireless/hotpot/issues?state=open",
    "source": "https://github.com/PGWireless/hotpot"
  },
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "php-msf": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-msf.git"
    },
    "php-log": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-log.git"
    },
    "php-inner-serv": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-inner-serv.git"
    },
    "php-helper": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-helper.git"
    },
    "php-context": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-context.git"
    },
    "php-aop": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-aop.git"
    },
    "php-db": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-db.git"
    },
    "php-filter": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-filter.git"
    },
    "php-exception": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-exception.git"
    },
    "php-third": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-third.git"
    },
    "php-i18n": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-i18n.git"
    },
    "pt-utils": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/pt-utils.git"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "PGWireless/php-msf": "dev-master"
  },
  "config": {
    "process-timeout": 1800,
    "vendor-dir": "./vendor/",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "extra": {
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Test\\": "test"
    }
  }
}
