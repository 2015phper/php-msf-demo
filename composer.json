{
  "name": "PGWireless/hotpot",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/PGWireless/hotpot/issues?state=open",
    "source": "https://github.com/PGWireless/hotpot"
  },
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "php-msf": {
      "type": "vcs",
      "url": "https://github.com/PGWireless/php-msf"
    },
    "php-log": {
      "type": "vcs",
      "url": "http://182.150.28.13:3000/PGWireless/php-log.git"
    },
    "php-helper": {
      "type": "vcs",
      "url": "http://182.150.28.13:3000/PGWireless/php-helper.git"
    },
    "php-exception": {
      "type": "vcs",
      "url": "http://182.150.28.13:3000/PGWireless/php-exception.git"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "PGWireless/php-msf": "dev-master"
  },
  "config": {
    "process-timeout": 1800,
    "vendor-dir": "./vendor/",
    "secure-http": false
  },
  "minimum-stability": "dev",
  "extra": {
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Test\\": "test"
    }
  }
}
